{
  "packageManager": "pnpm@9.10.0+sha256.355a8ab8dbb6ad41befbef39bc4fd6b5df85e12761d2724bd01f13e878de4b13",
  "name": "prigas",
  "description": "A CLI to guide with web applications development and deployment",
  "version": "0.0.3-beta.5",
  "author": "Victor Machado",
  "bin": {
    "prg": "./bin/run.js"
  },
  "bugs": "https://github.com/prigas-dev/prigas/issues",
  "dependencies": {
    "@inquirer/prompts": "^5.5.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/node": "^18",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^2.0.5",
    "vitest-mock-extended": "^2.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://prigas.dev",
  "keywords": [
    "oclif"
  ],
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "oclif": {
    "bin": "prg",
    "dirname": "prg",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "https://github.com/prigas-dev/prigas",
  "scripts": {
    "dev": "./bin/dev",
    "build": "tsc --project tsconfig.app.json",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "pnpm run build && oclif manifest && oclif readme",
    "test": "vitest",
    "version": "oclif readme && git add README.md"
  }
}
