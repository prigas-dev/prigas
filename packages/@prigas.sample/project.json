{
  "name": "@prigas/sample",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/@prigas.sample/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "cwd": "dist/@prigas/sample",
        "commands": ["npm publish --access public"],
        "parallel": false
      }
    },
    "build": {
      "executor": "tools.project:esbuild",
      "defaultConfiguration": "production",
      "dependsOn": ["clean"],
      "configurations": {
        "production": {
          "dev": false
        },
        "development": {
          "dev": true
        }
      }
    },
    "clean": {
      "executor": "tools.project:clean",
      "options": {
        "dir": "dist/@prigas/sample"
      }
    }
  }
}
